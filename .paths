# PostgreSQL
export DYLD_FALLBACK_LIBRARY_PATH=$HOME/lib:/usr/local/lib:/usr/lib:/Applications/Postgres.app/Contents/MacOS/lib

# PIP
export PIP_VIRTUALENV_BASE=$WORKON_HOME
export PIP_RESPECT_VIRTUALENV=true

# Timing
PROMPT_TITLE='echo -ne "\033]0;${USER}@${HOSTNAME%%.*}:${PWD/#$HOME/~}\007"'
export PROMPT_COMMAND="${PROMPT_COMMAND} ${PROMPT_TITLE}; "

# Default editor
export EDITOR='subl'

# Old PS1
# export PS1="\[\e[0;32m\]\u \w \n\$\[\e[0m\] "

# ssh
# export SSH_KEY_PATH="~/.ssh/dsa_id"

# compilation
# export ARCHFLAGS="-arch x86_64"

# vmrun binary
export PATH="$PATH:/Applications/VMware Fusion.app/Contents/Library"

# Virtualenvwrapper
export WORKON_HOME=$HOME/.virtualenvs
export PROJECT_HOME=$HOME/
source /usr/local/bin/virtualenvwrapper.sh

# GO
export GOPATH=$HOME/go/

# PostgreSQL
export PATH="$PATH:/Applications/Postgres.app/Contents/Versions/latest/bin"

# Heroku Toolbelt
export PATH="/usr/local/heroku/bin:$PATH"

# Terraform and packer
export PATH="/Users/alex/terraform:$PATH"
export PATH="/Users/alex/packer:$PATH"

# YARN
export PATH="$PATH:$HOME/.yarn/bin"

# OpenCV
export PATH="/usr/local/opt/opencv3/bin:$PATH"

# Fastlane
export PATH="$HOME/.fastlane/bin:$PATH"

# Code
export PATH="$PATH:/Applications/Visual Studio Code.app/Contents/Resources/app/bin"
